{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "oui-dialog-ex-demo-form",
  "type": "registry:component",
  "registryDependencies": [
    "https://oui.mw10013.workers.dev/r/oui-button.json",
    "https://oui.mw10013.workers.dev/r/oui-dialog.json",
    "https://oui.mw10013.workers.dev/r/oui-dialog-ex.json",
    "https://oui.mw10013.workers.dev/r/oui-heading.json",
    "https://oui.mw10013.workers.dev/r/oui-text-field-ex.json"
  ],
  "files": [
    {
      "path": "registry/components/oui-dialog-ex-demo-form.tsx",
      "content": "import { DialogEx } from \"@/registry/components/oui-dialog-ex\";\nimport { TextFieldEx } from \"@/registry/components/oui-text-field-ex\";\nimport { Button } from \"@/registry/components/ui/oui-button\";\nimport {\n  DialogDescription,\n  DialogFooter,\n  DialogHeader,\n} from \"@/registry/components/ui/oui-dialog\";\nimport { Heading } from \"@/registry/components/ui/oui-heading\";\nimport * as Rac from \"react-aria-components\";\n\nexport default function Component() {\n  return (\n    <DialogEx\n      triggerElement={<Button variant=\"outline\">DialogEx Form</Button>}\n      modalClassName=\"sm:max-w-[425px]\"\n    >\n      <Rac.Form>\n        <DialogHeader>\n          <Heading slot=\"title\">Edit profile</Heading>\n          <DialogDescription>\n            Make changes to your profile here. Click save when you&apos;re done.\n          </DialogDescription>\n        </DialogHeader>\n        <div className=\"grid gap-4 py-4\">\n          <TextFieldEx\n            name=\"name\"\n            label=\"Name\"\n            defaultValue=\"Pedro Duarte\"\n            autoFocus\n          />\n          <TextFieldEx\n            name=\"username\"\n            label=\"Username\"\n            defaultValue=\"@peduarte\"\n          />\n        </div>\n        <DialogFooter>\n          <Button variant=\"outline\" slot=\"close\">\n            Cancel\n          </Button>\n          <Button type=\"submit\" slot=\"close\">\n            Save changes\n          </Button>\n        </DialogFooter>\n      </Rac.Form>\n    </DialogEx>\n  );\n}\n",
      "type": "registry:component"
    }
  ],
  "meta": {
    "tags": [
      "dialog",
      "dialog-ex",
      "modal-ex",
      "button",
      "text-field-ex",
      "heading"
    ],
    "alignment": "flex-center"
  }
}