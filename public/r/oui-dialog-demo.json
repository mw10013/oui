{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "oui-dialog-demo",
  "type": "registry:component",
  "registryDependencies": [
    "@oui/oui-button",
    "@oui/oui-dialog",
    "@oui/oui-input",
    "@oui/oui-label",
    "@oui/oui-modal",
    "@oui/oui-text-field"
  ],
  "files": [
    {
      "path": "registry/default/components/oui-dialog-demo.tsx",
      "content": "\"use client\";\n\nimport { Button } from \"@/registry/default/ui/oui-button\";\nimport {\n  Dialog,\n  DialogDescription,\n  DialogFooter,\n  DialogHeader,\n  DialogTitle,\n} from \"@/registry/default/ui/oui-dialog\";\nimport { FieldError, FieldLabel } from \"@/registry/default/ui/oui-field\";\nimport { Input } from \"@/registry/default/ui/oui-input\";\nimport { Modal, ModalOverlay } from \"@/registry/default/ui/oui-modal\";\nimport { TextField } from \"@/registry/default/ui/oui-text-field\";\nimport * as Rac from \"react-aria-components\";\n\nexport default function Component() {\n  return (\n    <Rac.DialogTrigger>\n      <Button variant=\"outline\">Dialog</Button>\n      <ModalOverlay isDismissable>\n        <Modal className=\"sm:max-w-[425px]\">\n          <Dialog>\n            <Rac.Form>\n              <DialogHeader>\n                <DialogTitle>Edit profile</DialogTitle>\n                <DialogDescription>\n                  Make changes to your profile here. Click save when you&apos;re\n                  done.\n                </DialogDescription>\n              </DialogHeader>\n              <div className=\"grid gap-4 py-4\">\n                <TextField name=\"name\" autoFocus defaultValue=\"Pedro Duarte\">\n                  <FieldLabel>Name</FieldLabel>\n                  <Input />\n                  <FieldError />\n                </TextField>\n                <TextField name=\"username\" defaultValue=\"@peduarte\">\n                  <FieldLabel>Username</FieldLabel>\n                  <Input />\n                  <FieldError />\n                </TextField>\n              </div>\n              <DialogFooter>\n                <Button variant=\"outline\" slot=\"close\">\n                  Cancel\n                </Button>\n                <Button type=\"submit\" slot=\"close\">\n                  Save changes\n                </Button>\n              </DialogFooter>\n            </Rac.Form>\n          </Dialog>\n        </Modal>\n      </ModalOverlay>\n    </Rac.DialogTrigger>\n  );\n}\n",
      "type": "registry:component"
    }
  ],
  "meta": {
    "tags": [
      "dialog",
      "modal",
      "text-field"
    ],
    "canPreview": true,
    "alignment": "flex-center",
    "canOpenInV0": true
  }
}